
server {
    server_name black-hole.shop;

   location /static/ {
        alias /var/www/Asadbek/Asadbek/static/;
   }

   location /media/ {
        alias /var/www/Asadbek/Asadbek/media/;
   }


   location / {
        include proxy_params;
        proxy_pass http://unix:/var/www/Asadbek/Asadbek/Logistics.sock;
        # proxy_pass http://localhost:8000;
   }

    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/black-hole.shop/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/black-hole.shop/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot
}
server {
    listen 80;
    server_name black-hole.shop;

    # Redirect HTTP to HTTPS
    return 301 https://$host$request_uri;
}